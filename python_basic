#8. 리스트와 내장함수 -1
import random as r
a = [1,2,3,4,5]

b = list(range(1,11))
#print(b)
c = a+ b
#print(c)
a.append(6)
a.insert(3,7)
print(a)
a.pop(3)
print(a)
a.remove(4)
print(a)
print(a.index(5))
a = list(range(1,11))
print(a)

print(sum(a))
print(max(a))
print(min(a))
print(min(7,5,4,10))


r.shuffle(a)
print(a)
a.sort()
print(a)
a.sort(reverse = True)
print(a)
a.clear()
print(a)
