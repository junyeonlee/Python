'''
#9.리스트와 내장함수 -2
a = [23,12,36,53,19]
print(a[:3])
print(len(a))
for i in range(len(a)):
       print(a[i], end = ' ')

print()
for x in a :
       if x%2 ==1:
                     print(x, end = ' ')
print()
for x, y in enumerate(a):
       print(x,y)


if any(i>20 for i in a):
       print('yes')
else:
       print('no')
