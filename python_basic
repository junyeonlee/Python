'''
#7. 문자열과 내장함수
msg = "It is Time"
print(msg.upper())

tmp = msg.upper()
print(tmp.find('4'))

print(msg[0])
print(msg[:2])
print(msg[3:5])
print(len(msg))
for i in range(len(msg)):
       print(msg[i],end='')
print()
for x in msg :
       print(x, end = '')
print()
for x in msg:
       if x.islower():
              print(x, end = '')

tmp = 'AZ'
for x in tmp:
       print(ord(x))
print()

tmp = 'az'
for x in tmp :
       print(ord(x))

tmp = 100
print(chr(tmp))

