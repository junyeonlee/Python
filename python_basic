#2. 합계구하기
n = int(input())
sum = 0
for i in range(1,n+1):
       sum = sum + i
       if i == n:
              print(sum)

#3. N의 약수 출력하기
n = int(input())
list = 1
for i in range(1,n+1):
       if n%i == 0:
              print(i, end = ' ')

              # end = ' ', '\n' 잘 기억하자
